    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "minimist": "^1.2.8",
    "picocolors": "^1.1.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prompt": "^1.3.0",
    "prompts": "^2.4.2",
    "rslog": "^1.2.3",
    "ts-morph": "^25.0.1",
    "webpack": "^5.99.9",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4.3.20",
  "@types/estree": "^1.0.7",
    "@types/mocha": "^10",
    "@types/node": "^22.15.18",
    "@types/sinon": "^17.0.4",
    "@types/winston": "^2.4.4",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json",
    "./templates",
    "./scripts"
  ],
  "homepage": "https://github.com/lynx-family/lepo",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "lepo",
    "dirname": "lepo",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "lynx-family/lepo",
  "scripts": {
    "build": "shx rm -rf dist tsconfig.tsbuildinfo && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@7.33.6+sha512.90e27fd38047f18583f3342f784cc3f187f4d4caac89ebc1fffae18dcd7b2dde7678a0bf237481bcb8f7e8e66135fa34803856e4eb2c442ce082ffab5d9d241f",
  "pnpm": {
    "overrides": {
      "@types/estree": "1.0.7",
      "@types/eslint-scope": "8.3.2"
    }
  }
}
